{
  "name": "jsreport-reports",
  "version": "2.0.0",
  "description": "jsreport extension adding support for storing rendering outputs for later use",
  "scripts": {
    "start": "jsreport-studio-start --httpPort 5488",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "test": "mocha test --timeout=10000 && standard",
    "build": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish"
  },
  "homepage": "https://github.com/jsreport/jsreport-reports",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-reports.git"
  },
  "keywords": [
    "jsreport"
  ],
  "standard": {
    "ignore": [
      "main.js"
    ],
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  },
  "engines": {
    "node": ">=8.9"
  },
  "license": "LGPL",
  "dependencies": {
    "bluebird": "3.5.1",
    "lodash.omit": "4.5.0",
    "node.extend": "2.0.0"
  },
  "author": "Jan Blaha",
  "devDependencies": {
    "babel-eslint": "8.2.3",
    "eslint": "4.19.1",
    "eslint-config-standard-jsx": "5.0.0",
    "eslint-plugin-babel": "5.1.0",
    "in-publish": "2.0.0",
    "jsreport-core": "2.1.0",
    "jsreport-express": "2.2.0",
    "jsreport-studio-dev": "1.3.0",
    "mocha": "5.2.0",
    "should": "13.2.1",
    "standard": "11.0.1",
    "supertest": "3.1.0"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ]
}
